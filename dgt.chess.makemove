ord() { 
  LC_CTYPE=C printf '%d' "'$1" 
}

############################################################
# Put board into unabbreviated format, so we can figure out 
# which piece is moving.
############################################################

BOARD=`cat $2 | sed 's/ .*//' | sed 's/1/-/g' | sed 's/2/--/g' | sed 's/3/---/g' | sed 's/4/----/g' | sed 's/5/-----/g' | sed 's/6/------/g' | sed 's/7/-------/g' | sed 's/8/--------/g' | tr -d '/' | tr 'a-z' 'A-Z'` 

############################################################
# Strip promotion 'q' off of input move, if exists.
############################################################

MOVE=`echo $1 | awk '{printf("%s",substr($0,1,4))}'`

############################################################
# Pull source and destination out of MOVE
############################################################

SOURCE=`echo $MOVE | awk '{printf("%s",substr($0,1,2))}'`
SOURCEFILE=`echo $MOVE | awk '{printf("%s",substr($0,1,1))}'`
SOURCERANK=`echo $MOVE | awk '{printf("%s",substr($0,2,1))}'`

DEST=`echo $MOVE | awk '{printf("%s",substr($0,3,2))}'`
DESTFILE=`echo $MOVE | awk '{printf("%s",substr($0,3,1))}'`
DESTRANK=`echo $MOVE | awk '{printf("%s",substr($0,4,1))}'`

RANKDIFF=`echo '('$DESTRANK'-'$SOURCERANK')' | bc`

############################################################
# Figure out what piece type is in source square
############################################################

A=`ord $SOURCEFILE`
B=`echo '('$A'-96)+((8-'$SOURCERANK')*8)-1' | bc`
SOURCEPIECE=${BOARD:$B:1}

############################################################
# Figure out piece type in destination square (or - if empty)
############################################################

A=`ord $DESTFILE`
B=`echo '('$A'-96)+((8-'$DESTRANK')*8)-1' | bc`
DESTPIECE=${BOARD:$B:1}

############################################################
# Convert SOURCE & TARGET algebraic to square indices (1-64)
############################################################

SOURCENUM=`awk '{if ($1=="'$SOURCE'") print NR}' positions.names`
DESTNUM=`awk '{if ($1=="'$DEST'") print NR}' positions.names`

############################################################
# Display confirmation
############################################################

echo 'SOURCE='$SOURCE'  DEST='$DEST
echo 'SOURCEPIECE='$SOURCEPIECE'  DESTPIECE='$DESTPIECE
echo 'SOURCENUM='$SOURCENUM'  DESTNUM='$DESTNUM

############################################################
# Prepare grippers
############################################################

# Already prepared by dgt.chess.play!
# rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
# rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70

############################################################
# If move is a capture, remove enemy piece first
############################################################

if [[ $SOURCEPIECE == "N" ]]; then 
  SQUEEZE=25
  UNSQUEEZE=-2
elif [[ $SOURCEPIECE == "B" ]]; then
  SQUEEZE=20
  UNSQUEEZE=-2
else 
  SQUEEZE=20
  UNSQUEEZE=-3
fi

if [[ $DESTPIECE != "-" ]]; then

  echo 'Move to '$DEST

  moveto ${POSHOVER[$DESTNUM]}
  if [[ $DESTPIECE == "K" ]]; then moveto ${POSKING[$DESTNUM]}
  elif [[ $DESTPIECE == "B" ]]; then moveto ${POSBISHOP[$DESTNUM]}
  elif [[ $DESTPIECE == "P" ]]; then moveto ${POSPAWN[$DESTNUM]}
  elif [[ $DESTPIECE == "Q" ]]; then moveto ${POSQUEEN[$DESTNUM]}
  elif [[ $DESTPIECE == "N" ]]; then moveto ${POSKNIGHT[$DESTNUM]}
  elif [[ $DESTPIECE == "R" ]]; then moveto ${POSROOK[$DESTNUM]}
  fi

  echo 'Pick up '$DESTPIECE
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $SQUEEZE $SQUEEZE

  echo 'Move to bin'
  moveto ${POSHOVER[$DESTNUM]}
  moveto $NEUTRAL
  moveto $OFFBOARD

  echo 'Loosen grip & drop piece'
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $UNSQUEEZE $UNSQUEEZE
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -20 -20

  echo 'Prepare grippers and move to NEUTRAL'
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70
  moveto $NEUTRAL
fi

############################################################
# Move piece from source to destination
############################################################

if [[ $SOURCEPIECE == "N" ]]; then 
  SQUEEZE=25
  UNSQUEEZE=-2
elif [[ $SOURCEPIECE == "B" ]]; then
  SQUEEZE=20
  UNSQUEEZE=-2
else 
  SQUEEZE=20
  UNSQUEEZE=-3
fi

echo 'Move to '$SOURCE

moveto ${POSHOVER[$SOURCENUM]}
if [[ $SOURCEPIECE == "K" ]]; then moveto ${POSKING[$SOURCENUM]}
elif [[ $SOURCEPIECE == "B" ]]; then moveto ${POSBISHOP[$SOURCENUM]}
elif [[ $SOURCEPIECE == "P" ]]; then moveto ${POSPAWN[$SOURCENUM]}
elif [[ $SOURCEPIECE == "Q" ]]; then moveto ${POSQUEEN[$SOURCENUM]}
elif [[ $SOURCEPIECE == "N" ]]; then moveto ${POSKNIGHT[$SOURCENUM]}
elif [[ $SOURCEPIECE == "R" ]]; then moveto ${POSROOK[$SOURCENUM]}
fi

echo 'Pick up '$SOURCEPIECE

rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $SQUEEZE $SQUEEZE

echo 'Move to '$DEST' area'

moveto ${POSHOVER[$SOURCENUM]}

if [[ $RANKDIFF -gt 3 || ( $RANKDIFF -gt 2 && $DESTRANK -gt 6 ) ]]; then moveto.134.2 ${POSHOVER[$DESTNUM]}
else moveto ${POSHOVER[$DESTNUM]}
fi

echo 'Loosen grip'

rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $UNSQUEEZE $UNSQUEEZE

echo 'Lower piece to '$DESTNUM

if [[ $SOURCEPIECE == "K" ]]; then moveto ${POSKING[$DESTNUM]}
elif [[ $SOURCEPIECE == "B" ]]; then moveto ${POSBISHOP[$DESTNUM]}
elif [[ $SOURCEPIECE == "P" ]]; then moveto ${POSPAWN[$DESTNUM]}
elif [[ $SOURCEPIECE == "Q" ]]; then moveto ${POSQUEEN[$DESTNUM]}
elif [[ $SOURCEPIECE == "N" ]]; then moveto ${POSKNIGHT[$DESTNUM]}
elif [[ $SOURCEPIECE == "R" ]]; then moveto ${POSROOK[$DESTNUM]}
fi

echo 'Drop piece'

rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -20 -20

echo 'Record new position'

dgt.chess.getboard dgt.chess.nextpos

echo 'Move arm up'

moveto ${POSHOVER[$DESTNUM]}

############################################################
# If promotion, "capture own pawn with offboard queen"
############################################################

if [ "$DESTRANK" == 8 ] && [ "$SOURCEPIECE" == "P" ]; then
#
# move robot's pawn offboard
#
  SQUEEZE=20
  UNSQUEEZE=-4
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70
  moveto ${POSPAWN[$DESTNUM]}
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $SQUEEZE $SQUEEZE
  moveto ${POSHOVER[$DESTNUM]}
  moveto $NEUTRAL
  moveto $OFFBOARD
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $UNSQUEEZE $UNSQUEEZE
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -20 -20
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70
#
# grab promotion queen
#
  moveto $PROMOTIONHOVER
  moveto $PROMOTIONQUEEN
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $SQUEEZE $SQUEEZE
  moveto $PROMOTIONHOVER
#
# move promotion queen to where pawn was
#
  moveto ${POSHOVER[$DESTNUM]}
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- $UNSQUEEZE $UNSQUEEZE
  moveto ${POSQUEEN[$DESTNUM]}
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -20 -20
  dgt.chess.getboard dgt.chess.nextpos
  moveto ${POSHOVER[$DESTNUM]}
fi

############################################################
# If castle, recursively move rook.  Otherwise, return to
# NEUTRAL position.
############################################################

if [[ $1 == "e1g1" ]]; then
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70
  dgt.chess.makemove h1f1 $2
elif [[ $1 == "e1c1" ]]; then
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70
  dgt.chess.makemove a1d1 $2
else
  moveto $NEUTRAL
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- -100 -100
  rosrun kinova_demo fingers_action_client.py -r m1n4s200 percent -- 70 70
fi

